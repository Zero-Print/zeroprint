{
  "name": "zeroprint-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:auto": "testsprite generate --framework=jest",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:security": "playwright test tests/security/security-tests.spec.ts",
    "test:security:dpdp": "jest --testPathPattern=tests/security/dpdp-compliance.test.ts",
    "test:security:pentest": "playwright test tests/security/penetration-tests.spec.ts",
    "test:load": "k6 run tests/performance/load-tests.js",
    "test:stress": "k6 run tests/performance/stress-tests.js",
    "test:spike": "k6 run tests/performance/spike-tests.js",
    "test:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:accessibility": "playwright test tests/accessibility/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "emulators": "firebase emulators:start --import=emulator-data --export-on-exit",
    "emulators:ui": "firebase emulators:start --import=emulator-data --export-on-exit --inspect-functions",
    "seed:emulator": "tsx scripts/seed-test-data.ts",
    "k6": "k6 run",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "bundle-analyzer": "@next/bundle-analyzer",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run test:ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "firebase": "^10.11.0",
    "lucide-react": "^0.294.0",
    "next": "^14.2.32",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.3.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^2.0.0",
    "tw-animate-css": "^1.3.8",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@firebase/rules-unit-testing": "^3.0.0",
    "@next/bundle-analyzer": "^14.0.0",
    "@next/swc-win32-x64-msvc": "^14.2.33",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/testing-library": "^0.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@testsprite/testsprite-mcp": "^0.0.14",
    "@types/jest": "^30.0.0",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "10.4.16",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-security": "^1.7.0",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^12.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^3.0.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.11.3",
    "postcss": "8.4.31",
    "prettier": "^3.1.0",
    "react-hook-form": "^7.62.0",
    "storybook": "^8.6.14",
    "tailwindcss": "3.3.5",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0",
    "typescript": "5.2.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "whatwg-fetch": "^3.6.0"
  }
}
